{
  "name": "ejlog-react-webapp",
  "version": "1.0.0",
  "description": "EjLog WMS - Modern React WebApp with Ferretto Theme",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server/clear-cache-and-start.js",
    "start:nocache": "node server/start-all-real-data.js",
    "start:full": "concurrently \"npm run start:https\" \"npm run start:backend-sql\" \"npm run start:backend-react\" \"npm run dev:frontend-only\" --names \"HTTPS-3079,SQL-3077,REACT-8080,VITE-3000\" --prefix-colors \"magenta,cyan,blue,green\" --kill-others-on-fail",
    "start:https": "cross-env HTTPS_PORT=3079 node server/api-server-https.js",
    "start:backend-sql": "cross-env PORT=3077 node server/api-server.js",
    "start:backend-react": "cross-env PORT=8080 node server/api-server.js",
    "dev": "concurrently \"npm run dev:backend-3077\" \"npm run dev:backend-9000\" \"npm run dev:frontend-only\" --names \"SQL-3077,TEMPLATES-9000,VITE-3000\" --prefix-colors \"cyan,magenta,green\" --kill-others-on-fail",
    "dev:frontend-only": "vite",
    "dev:backend-3077": "node server/api-server.js",
    "dev:backend-9000": "cd ../../../ejlog-rest-backend && set PORT=9000 && npm run dev",
    "dev:api": "node server/api-server.js",
    "dev:api:https": "node server/api-server-https.js",
    "dev:api:legacy": "node server/proxy-api.js",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev:frontend-only\" --names \"API,VITE\" --prefix-colors \"cyan,green\"",
    "dev:all:https": "concurrently \"npm run dev:api:https\" \"npm run dev:frontend-only\" --names \"API-HTTPS,VITE\" --prefix-colors \"magenta,green\"",
    "api": "node server/api-server.js",
    "api:https": "node server/api-server-https.js",
    "api:legacy": "node server/proxy-api.js",
    "generate:certs": "node server/generate-ssl-certs.js",
    "test:db": "node server/test-connection.js",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:e2e:codegen": "playwright codegen http://localhost:3004",
    "mock": "node mock-api-server.js",
    "deploy": "node deploy.js",
    "deploy:staging": "node deploy.js --staging",
    "deploy:quick": "node deploy.js --skip-tests",
    "deploy:vercel": "node deploy.js --platform vercel",
    "verify": "node verify-deployment.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@mui/x-data-grid": "^8.22.1",
    "@react-three/drei": "^9.88.0",
    "@react-three/fiber": "^8.15.0",
    "@reduxjs/toolkit": "^2.10.1",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.24",
    "html5-qrcode": "^2.3.8",
    "i18next": "^25.7.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.3",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "konva": "^9.3.15",
    "lucide-react": "^0.323.0",
    "node-cron": "^4.2.1",
    "node-forge": "^1.3.3",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-layout": "^1.5.2",
    "react-hook-form": "^7.67.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^16.5.0",
    "react-konva": "^18.2.10",
    "react-redux": "^9.2.0",
    "react-resizable": "^3.0.5",
    "react-router-dom": "^6.22.0",
    "recharts": "^2.15.4",
    "selfsigned": "^5.2.0",
    "socket.io-client": "^4.7.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.1",
    "three": "^0.158.0",
    "vite-plugin-pwa": "^0.19.8",
    "web-vitals": "^4.2.4",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5",
    "zod": "^4.1.13",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/papaparse": "^5.5.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "express": "^4.18.2",
    "express-validator": "^7.3.1",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "mssql": "^11.0.1",
    "playwright": "^1.56.1",
    "postcss": "^8.4.35",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.9.3",
    "vite": "^5.1.0",
    "vitest": "^1.2.2",
    "yamljs": "^0.3.0"
  }
}

